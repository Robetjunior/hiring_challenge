{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "yarn run generate && ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "tsoa": "tsoa spec-and-routes",
    "generate": "yarn tsoa && yarn build",
    "test": "yarn run generate && jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migration:generate": "ts-node-dev --respawn ./node_modules/typeorm/cli.js migration:generate \"src/migrations/CreateAreaNeighborsAndEquipmentAreas\" --dataSource \"src/config/database.ts\"",
    "migration:run":      "ts-node-dev --respawn ./node_modules/typeorm/cli.js migration:run --dataSource \"src/config/database.ts\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^6.6.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.24",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.6",
    "jest": "^29.7.0",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
